          "command": "execute at @s positioned ~ ~0.7 ~ run execute positioned ^-0.4 ^ ^ facing entity @e[tag=GRABBED,limit=1] feet run tp @e[tag=THROW] ^ ^ ^ ~ ~-5"
        },
        {
          "type": "origins:execute_command",
          "command": "tp @e[tag=GRABBED] ^ ^1 ^9"
        },
        {
          "type": "origins:execute_command",
