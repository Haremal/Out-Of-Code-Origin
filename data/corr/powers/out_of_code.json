{
  "type": "origins:multiple",
  "relocate": {
    "type": "origins:active_self",
    "entity_action": {
      "type": "origins:delay",
      "ticks": 10,
      "action": {
        "type": "origins:and",
        "actions": [
          {
            "type": "origins:execute_command",
            "command": "tag @s add CORR_CLONE"
          },
          {
            "type": "origins:execute_command",
            "command": "scoreboard players set ClonesExist Timer 0"
          },
          {
            "type": "origins:execute_command",
            "command": "execute unless score ClonesCount Timer matches 5.. at @e[nbt={Brain:{}},tag=!NAN,distance=..15,limit=1,sort=nearest] run summon minecraft:armor_stand ~ ~ ~ {Tags:[\"NAN\",\"CORR_CLONE\"],NoBasePlate:true,ArmorItems:[{id:\"leather_boots\",Count:1,tag:{display:{color:0}}},{id:\"leather_leggings\",Count:1,tag:{display:{color:0}}},{id:\"leather_chestplate\",Count:1,tag:{display:{color:0}}},{id:\"leather_helmet\",Count:1,tag:{display:{color:8991416}}}]}"
          },
          {
            "type": "origins:execute_command",
            "command": "execute at @e[nbt={Brain:{}},tag=!NAN,distance=..15,limit=1,sort=nearest] run spreadplayers ~ ~ 0 7 false @e[tag=CORR_CLONE]"
          },
          {
            "type": "origins:execute_command",
            "command": "execute as @e[tag=CORR_CLONE] at @s run tp @s ~ ~ ~ facing entity @e[nbt={Brain:{}},tag=!NAN,distance=..15,limit=1,sort=nearest]"
          },
          {
            "type": "origins:execute_command",
            "command": "execute if entity @e[nbt={Brain:{}},tag=!NAN,distance=..15,limit=1,sort=nearest] run item replace entity @e[tag=CORR_CLONE] armor.head with minecraft:leather_helmet{display:{color:8991416}}"
          },
          {
            "type": "origins:execute_command",
            "command": "execute if entity @e[nbt={Brain:{}},tag=!NAN,distance=..15,limit=1,sort=nearest] run stopsound @a[distance=..15] * minecraft:item.armor.equip_leather"
          }
        ]
      }
    },
    "key": {
      "key": "key.origins.secondary_active",
      "continuous": true
    },
    "condition": {
      "type": "origins:and",
      "conditions": [
        {
          "type": "origins:sneaking",
          "inverted": true
        },
        {
          "type": "origins:command",
          "command": "execute if entity @e[tag=GRABBED]",
          "comparison": "==",
          "compare_to": 0
        }
      ]
    }
  },
  "relocate_setup": {
    "type": "origins:active_self",
    "entity_action": {
      "type": "origins:play_sound",
      "sound": "minecraft:entity.experience_orb.pickup"
    },
    "key": {
      "key": "key.origins.secondary_active",
      "continuous": false
    },
    "condition": {
      "type": "origins:and",
      "conditions": [
        {
          "type": "origins:sneaking",
          "inverted": true
        },
        {
          "type": "origins:command",
          "command": "execute if entity @e[tag=GRABBED]",
          "comparison": "==",
          "compare_to": 0
        },
        {
          "type": "origins:command",
          "command": "execute if entity @e[nbt={Brain:{}},tag=!NAN,distance=..15,limit=1,sort=nearest]",
          "comparison": "==",
          "compare_to": 1
        }
      ]
    }
  },

  "glitch": {
    "type": "origins:active_self",
    "entity_action": {
      "type": "origins:if_else_list",
      "actions": [
        {
          "condition": {
            "type": "origins:and",
            "conditions": [
              {
                "type": "origins:dimension",
                "dimension": "corr:out_of_code_dimension",
                "inverted": true
              },
              {
                "type": "origins:command",
                "command": "execute if entity @e[tag=GRABBED]",
                "comparison": "==",
                "compare_to": 0
              }
            ]
          },
          "action": {
            "type": "origins:and",
            "actions": [
              {
                "type": "origins:execute_command",
                "command": "execute in corr:out_of_code_dimension run tp @s[tag=!CORR_NERVES] ~ ~ ~"
              },
              {
                "type": "origins:execute_command",
                "command": "spreadplayers ~ ~ 0 1 false @s[tag=!CORR_NERVES]"
              },
              {
                "type": "origins:execute_command",
                "command": "execute as @e[tag=!NAN,tag=NAN_BOTTOM] at @s in corr:out_of_code_dimension run tp @s ~ 1 ~"
              },
              {
                "type": "origins:execute_command",
                "command": "kill @e[tag=CORR_NERVES,type=!player,scores={Timer=5..}]"
              },
              {
                "type": "origins:execute_command",
                "command": "tag @e[tag=NAN_BOTTOM,tag=!NAN] remove NAN_BOTTOM"
              },
              {
                "type": "origins:execute_command",
                "command": "tag @e[type=!player,tag=NAN,tag=!THROW] add REMOVE"
              }
            ]
          }
        },
        {
          "condition": {
            "type": "origins:dimension",
            "dimension": "corr:out_of_code_dimension"
          },
          "action": {
            "type": "origins:and",
            "actions": [
              {
                "type": "origins:execute_command",
                "command": "execute if score corrDim Position matches 0 run execute in minecraft:overworld run tp @s ~ ~ ~"
              },
              {
                "type": "origins:execute_command",
                "command": "execute if score corrDim Position matches 1 run execute in minecraft:the_nether run tp @s ~ ~ ~"
              },
              {
                "type": "origins:execute_command",
                "command": "execute if score corrDim Position matches 2 run execute in minecraft:the_end run tp @s ~ ~ ~"
              },
              {
                "type": "origins:execute_command",
                "command": "spreadplayers ~ ~ 0 1 false @s"
              }
            ]
          }
        }
      ]
    },
    "cooldown": 3,
    "key": {
      "key": "key.origins.secondary_active",
      "continuous": true
    },
    "condition": {
      "type": "origins:sneaking"
    }
  },

  "spawn": {
    "type": "origins:modify_player_spawn",
    "dimension": "corr:out_of_code_dimension",
    "spawn_strategy": "center"
